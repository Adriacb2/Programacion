Proceso sin_titulo
	Definir a Como Entero
	Definir b Como Entero
	Escribir "Dime un número"
	Leer a
	Escribir "Dime otro número"
	leer b
	si a o b > 0 Entonces
		//El programa no me reconoce el comando "y" u "o" para realizar la unión de a y b.
		Escribir "El primer número es positivo"
	SiNo
		Escribir "El primer número es negativo"
		Si b > 0 Entonces
			Escribir " El segundo número es positivo"
		SiNo
			Escribir "El primer número es negativo"
		FinSi
	FinSi
FinProceso
